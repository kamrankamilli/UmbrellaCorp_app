<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.umbrellacorp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.example.umbrellacorp.ui.dashboard.DashboardFragment"
        android:label="@string/title_company_history"
        tools:layout="@layout/frag_history" />

    <fragment
        android:id="@+id/navigation_vision"
        android:name="com.example.umbrellacorp.ui.notifications.NotificationsFragment"
        android:label="@string/title_vision"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_contact"
        android:name="com.example.umbrellacorp.ui.notifications.NotificationsFragment"
        android:label="@string/title_contact"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_support"
        android:name="com.example.umbrellacorp.ui.notifications.NotificationsFragment"
        android:label="@string/title_support"
        tools:layout="@layout/frag_support" />
</navigation>

